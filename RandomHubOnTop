function tween(position)
  Distance = (position.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude
  game:GetService("TweenService"):Create(
      game.Players.LocalPlayer.Character.HumanoidRootPart,
      TweenInfo.new(Distance / 500, Enum.EasingStyle.Linear),
      {CFrame = position}
  ):Play()
end
--table
local mobs = {}
 for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do 
  
  table.insert(mobs, v.Name)
 end


local players = {}
for i,v in pairs(game:GetService("Players"):GetChildren()) do
  if v.ClassName == "Player" then
     table.insert(players,v.Name)
  end
end

local npcs = {}
for i,v in pairs(game.Workspace:GetChildren()) do
  if v:FindFirstChild("Humanoid") and v:FindFirstChild("ProximityPrompt") then
     table.insert(npcs,v.Name)
  end
end


local anticheat
anticheat = hookmetamethod(game, "__index", newcclosure(function(...)
    local self, k = ...
    
    if not checkcaller() and k == "WalkSpeed" and self.Name == "Humanoid" and self:IsA("Humanoid") then
        return 16;
    elseif not checkcaller() and k == "JumpPower" and self.Name == "Humanoid" and self:IsA("Humanoid") then
        return 50;
    elseif not checkcaller() and k == "Gravity" and self.Name == "Workspace" then
        return 196.2;
    end
    
    return anticheat(...)
end))

local Plr = game.Players.LocalPlayer

-- Part of Anti Cheat Bypass
local antikick
antikick = hookmetamethod(game, "__namecall", function(...)
    local self, k = ...

    if not checkcaller() and self == Plr and k == "Kick" then
        return;
    end
    
    return antikick(...)
end)




